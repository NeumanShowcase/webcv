<div class="container-fluid p-0">
    <section class="resume-section p-3 p-lg-5 d-flex align-items-center" id="about">
        <div class="w-100">
            <img class=".img-rounded .float-left d-block" style="width:40%" src="img/profile.jpg" alt="">
            <h1 class="mb-0">Oskar
                <span class="text-primary">Neuman</span>
            </h1>
            <div class="subheading mb-5">{{About.Phone}} <a
                    href="https://www.thetimezoneconverter.com/?t=8%3A00%20am&tz=Sweden&">Between 8 am and 7 pm</a><br>

                <a href="mailto:{{About.Email}}">{{About.Email}}</a>
            </div>
            <p class="lead mb-5">{{About.Desc}}</p>
            <div class="social-icons">
                <a href="#">
                    <i class="fab fa-linkedin-in"></i>
                </a>
                <a href="https://github.com/{{About.GithubUsername}}">
                    <i class="fab fa-github"></i>
                </a>
            </div>
        </div>
    </section>
</div>